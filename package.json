		"@mantine/hooks": "^4.2.6",
		"@mantine/next": "^4.2.6",
		"@mantine/notifications": "^4.2.6",
		"@mantine/prism": "^4.2.6",
		"axios": "^0.27.2",
		"cookies-next": "^2.0.4",
		"dayjs": "^1.11.2",
		"framer-motion": "^6.3.1",
		"js-file-download": "^0.4.12",
		"next": "12.1.6",
		"prism-react-renderer": "^1.3.1",
		"react": "^17.0.1",
